<UserControl x:Class="EngineCell.Application.Views.Clocks.Clock"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EngineCell.Application.Views"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Width="112" Margin="0,5,0,5">
        <TextBox IsUndoEnabled="False" Text="{Binding TimeOfDay}" IsEnabled="False" IsReadOnly="True" FontSize="20" Width="90" Height="35" HorizontalAlignment="Right" BorderBrush="Transparent"></TextBox>
    </Grid>
</UserControl>
