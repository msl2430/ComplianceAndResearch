<UserControl x:Class="EngineCell.Application.Views.Clocks.RunTime"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EngineCell.Application.Views.Clocks"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid Width="270" Margin="0,5,0,5">
            <Label Width="120" Height="45" FontSize="25"  HorizontalAlignment="Right" Margin="0,0,110,0"  VerticalAlignment="Center">Run Time:</Label>
            <TextBox Text="{Binding RunTime}" IsReadOnly="True" FontSize="25" Width="110" Height="40" HorizontalAlignment="Right" BorderBrush="Transparent"></TextBox>
        </Grid>
    </Grid>
</UserControl>
